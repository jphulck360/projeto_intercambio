<div class="page-header">
  <%= link_to schedules_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_schedule_path(@schedule), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show schedule</h1>
</div>

<dl class="dl-horizontal">
  <dt>Dt saida:</dt>
  <dd><%= @schedule.dt_saida %></dd>

  <dt>Dt retorno:</dt>
  <dd><%= @schedule.dt_retorno %></dd>

  <dt>Dias:</dt>
  <dd><%= @schedule.dias %></dd>

  <dt>Preco:</dt>
  <dd><%= @schedule.preco %></dd>

  <dt>Obs:</dt>
  <dd><%= @schedule.obs %></dd>

</dl>
