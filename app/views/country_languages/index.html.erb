<p id="notice"><%= notice %></p>

<h1>Country Languages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @country_languages.each do |country_language| %>
      <tr>
        <td><%= link_to 'Show', country_language %></td>
        <td><%= link_to 'Edit', edit_country_language_path(country_language) %></td>
        <td><%= link_to 'Destroy', country_language, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country Language', new_country_language_path %>
